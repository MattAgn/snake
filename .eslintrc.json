{
  "plugins": ["prettier"],
  "extends": "airbnb",
  "rules": {
    "react/jsx-filename-extension": "off",
    "no-underscore-dangle": "off",
    "no-plusplus": "off",
    "comma-dangle": "off",
    "prettier/prettier": "error",
    "react/no-unused-prop-types": "off",
    "no-use-before-define": "off"
  }
}
